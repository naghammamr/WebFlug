@model IEnumerable<WebFlug.Models.FAQ>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Question)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Answer)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Question)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Answer)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FAQ_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FAQ_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FAQ_ID })
        </td>
    </tr>
}

</table>
